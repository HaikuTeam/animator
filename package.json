{
  "name": "haiku-mono",
  "private": true,
  "workspaces": [
    "packages/@haiku/*",
    "packages/haiku-*"
  ],
  "version": "3.1.23",
  "description": "Haiku monorepo",
  "main": "index.js",
  "author": "Matthew Trost <matthew@trost.co>",
  "license": "UNLICENSED",
  "scripts": {
    "test-url": "node ./scripts/test-url.js",
    "add-experiment": "node ./scripts/add-experiment.js",
    "republish-projects": "node ./scripts/republish-projects.js",
    "changelog": "node ./scripts/changelog.js",
    "doctor": "node ./scripts/doctor.js",
    "go": "yarn start default",
    "lint": "standard \"./scripts/**/*.js\" --fix --verbose | snazzy",
    "push": "node ./scripts/push.js",
    "semver": "node ./scripts/semver.js",
    "setup": "node ./scripts/setup.js",
    "start": "node ./scripts/start.js",
    "test": "yarn compile-all && ./test/run.sh",
    "compile-all": "node ./scripts/compile-all.js",
    "lint-all": "node ./scripts/lint-all.js",
    "test-all": "node ./scripts/test-all.js",
    "watch-all": "node ./scripts/watch-all.js",
    "lint-report": "node ./scripts/lint-report.js",
    "test-report": "node ./scripts/test-report.js"
  },
  "dependencies": {
    "@haiku/player": "^3.0.4",
    "@haiku/cli": "3.1.23",
    "@haiku/core": "3.1.23",
    "@haiku/sdk-client": "3.1.23",
    "@haiku/sdk-inkstone": "3.1.23",
    "haiku-bytecode": "3.1.23",
    "haiku-common": "3.1.23",
    "haiku-creator": "3.1.23",
    "haiku-formats": "3.1.23",
    "haiku-fs-extra": "3.1.23",
    "haiku-glass": "3.1.23",
    "haiku-plumbing": "3.1.23",
    "haiku-sdk-creator": "3.1.23",
    "haiku-serialization": "3.1.23",
    "haiku-state-object": "3.1.23",
    "haiku-timeline": "3.1.23",
    "haiku-ui-common": "3.1.23",
    "haiku-websockets": "3.1.23"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "aws-sdk": "^2.85.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^2.2.0",
    "cli-color": "^1.2.0",
    "dependency-graph": "^0.5.0",
    "derequire": "^2.0.6",
    "dotenv": "^4.0.0",
    "electron": "1.8.2",
    "electron-builder": "20.0.8",
    "electron-rebuild": "^1.6.0",
    "fs-extra": "^4.0.2",
    "git-branch": "0.3.0",
    "glob": "^7.1.2",
    "glob-all": "3.1.0",
    "handlebars": "^4.0.8",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.17",
    "moment": "2.18.1",
    "node-gyp": "^3.6.2",
    "semver": "^5.4.1",
    "semver-sort": "^0.0.4",
    "slack": "^8.3.1",
    "snazzy": "^7.0.0",
    "spectron": "^3.7.2",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "typescript": "^2.5.2",
    "uglify-es": "^3.3.2",
    "yargs": "^6.6.0"
  },
  "standard": {
    "ignore": [
      "/packages/*"
    ]
  },
  "build": {
    "appId": "com.Haiku.HaikuForDesignersAndEngineers",
    "copyright": "Copyright (c) 2017 Haiku",
    "directories": {
      "app": "source"
    },
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "productName": "Haiku",
    "forceCodeSigning": true,
    "electronVersion": "1.8.2",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "protocols": {
      "name": "haiku",
      "schemes": [
        "haiku"
      ]
    },
    "asar": true,
    "asarUnpack": [
      "node_modules/haiku-plumbing/bins/*"
    ],
    "extraResources": [
      "app/index.js"
    ],
    "files": [
      "package.json",
      "node_modules/**/*",
      "changelog/public/*",
      "*.js",
      "!**/{README,readme,test,__tests__,tests,powered-test,example,examples,demo,demos}",
      "!node_modules/.bin",
      "!**/*.{md,iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj,ts,map,d.ts,idea}",
      "!**/.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,yarn-debug.log,.nyc_output,yarn.lock,.yarn-integrity,tslint*.json,tsconfig.json}",
      "!node_modules/eslint*",
      "!node_modules/monaco-editor/dev",
      "!node_modules/mout",
      "!node_modules/proxyquire",
      "!node_modules/sinon",
      "!node_modules/table",
      "!node_modules/nodegit/vendor",
      "!node_modules/haiku-plumbing/dev${/*}",
      "!node_modules/haiku-plumbing/docs${/*}",
      "!node_modules/haiku-plumbing/src${/*}",
      "!node_modules/haiku-plumbing/test${/*}",
      "!node_modules/@haiku/cli/src${/*}",
      "!node_modules/haiku-creator/src${/*}",
      "!node_modules/haiku-glass/src${/*}",
      "!node_modules/@haiku/sdk-client/src${/*}",
      "!node_modules/haiku-sdk-creator/src${/*}",
      "!node_modules/@haiku/sdk-inkstone/src${/*}",
      "!node_modules/haiku-timeline/src${/*}",
      "!node_modules/haiku-testing${/*}",
      "!node_modules/haiku-admin-cli${/*}",
      "!node_modules/haiku-ui-common/src${/*}",
      "!node_modules/@haiku/core/dist${/*}",
      "!node_modules/@haiku/core/src${/*}",
      "!node_modules/@haiku/core/test${/*}"
    ]
  }
}
