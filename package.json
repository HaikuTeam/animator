{
  "name": "haiku-mono",
  "private": true,
  "workspaces": [
    "packages/@haiku/*",
    "packages/haiku-*"
  ],
  "version": "2.3.64",
  "description": "Haiku monorepo",
  "main": "index.js",
  "author": "Matthew Trost <matthew@trost.co>",
  "license": "UNLICENSED",
  "scripts": {
    "doctor": "node ./scripts/doctor.js",
    "go": "yarn start default",
    "setup": "node ./scripts/setup.js",
    "changelog": "node ./scripts/changelog.js",
    "start": "node ./scripts/start.js",
    "sync": "echo 'Deprecated! Use `yarn install`.'",
    "lint": "standard \"./scripts/**/*.js\" --fix --verbose | snazzy",
    "semver": "node ./scripts/semver.js",
    "lint-all": "node ./scripts/lint-all.js",
    "test-all": "node ./scripts/test-all.js",
    "push": "node ./scripts/push.js",
    "compile-all": "node ./scripts/compile-all.js",
    "distro": "node ./scripts/distro.js",
    "test": "tape \"test/**/*.test.js\" | tap-spec"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "aws-sdk": "^2.85.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^2.2.0",
    "browserify": "^14.4.0",
    "cli-color": "^1.2.0",
    "dependency-graph": "^0.5.0",
    "derequire": "^2.0.6",
    "electron-builder": "19.49.0",
    "electron-rebuild": "^1.6.0",
    "fs-extra": "^4.0.2",
    "git-branch": "0.3.0",
    "glob": "^7.1.2",
    "glob-all": "3.1.0",
    "handlebars": "^4.0.8",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.17",
    "moment": "2.18.1",
    "node-gyp": "^3.6.2",
    "semver": "^5.4.1",
    "semver-sort": "^0.0.4",
    "slack": "^8.3.1",
    "snazzy": "^7.0.0",
    "spectron": "^3.7.2",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "uglify-es": "^3.3.2",
    "yargs": "^6.6.0"
  },
  "standard": {
    "ignore": [
      "/packages/*"
    ]
  },
  "dependencies": {},
  "build": {
    "appId": "com.Haiku.HaikuForDesignersAndEngineers",
    "copyright": "Copyright (c) 2017 Haiku",
    "directories": {
      "app": "source"
    },
    "productName": "Haiku",
    "forceCodeSigning": true,
    "electronVersion": "1.7.10",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "zip"
    },
    "asar": true,
    "asarUnpack": [
      "config.js",
      "packages/haiku-plumbing/bins/*"
    ],
    "files": [
      "**/*",
      "!**/{README,readme,test,__tests__,tests,powered-test,example,examples,demo}",
      "!**/node_modules/.bin",
      "!**/*.{md,iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj,ts,map,d.ts}",
      "!**/._*",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,yarn-debug.log,.nyc_output,yarn.lock,.yarn-integrity,tslint*.json,tsconfig.json}",
      "!**/@haiku/player/demo",
      "!node_modules/aws-sdk",
      "!node_modules/cli-table2",
      "!node_modules/eslint",
      "!node_modules/monaco-editor/dev",
      "!node_modules/mout",
      "!node_modules/node-pre-gyp",
      "!node_modules/rocambole-indent",
      "!node_modules/sinon",
      "!node_modules/table",
      "!node_modules/nodegit",
      "node_modules/nodegit/package.json",
      "node_modules/nodegit/dist/**",
      "node_modules/nodegit/build/Release/nodegit.node",
      "!**/haiku-plumbing/dev${/*}",
      "!**/haiku-plumbing/docs${/*}",
      "!**/haiku-plumbing/src${/*}",
      "!**/haiku-plumbing/test${/*}",
      "!**/@haiku/cli/src${/*}",
      "!**/haiku-creator/src${/*}",
      "!**/haiku-glass/src${/*}",
      "!**/@haiku/sdk-client/src${/*}",
      "!**/haiku-sdk-creator/src${/*}",
      "!**/@haiku/sdk-inkstone/src${/*}",
      "!**/haiku-timeline/src${/*}",
      "!**/haiku-testing${/*}",
      "!**/haiku-ui-common/src${/*}",
      "!**/@haiku/player/dist${/*}",
      "!**/@haiku/player/src${/*}",
      "!**/@haiku/player/test${/*}"
    ]
  }
}
