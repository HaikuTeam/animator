{
  "name": "haiku-mono",
  "private": true,
  "workspaces": [
    "packages/@haiku/*",
    "packages/haiku-*"
  ],
  "version": "2.3.61",
  "description": "Haiku monorepo",
  "main": "index.js",
  "author": "Matthew Trost <matthew@trost.co>",
  "license": "UNLICENSED",
  "scripts": {
    "doctor": "node ./scripts/doctor.js",
    "go": "yarn start default",
    "setup": "node ./scripts/setup.js",
    "changelog": "node ./scripts/changelog.js",
    "start": "node ./scripts/start.js",
    "sync": "echo 'Deprecated! Use `yarn install`.'",
    "lint": "standard \"./scripts/**/*.js\" --fix --verbose | snazzy",
    "semver": "node ./scripts/semver.js",
    "lint-all": "node ./scripts/lint-all.js",
    "test-all": "node ./scripts/test-all.js",
    "push": "node ./scripts/push.js",
    "compile-all": "node ./scripts/compile-all.js",
    "distro": "node ./scripts/distro.js",
    "test": "tape \"test/**/*.test.js\" | tap-spec"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "aws-sdk": "^2.85.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^2.2.0",
    "browserify": "^14.4.0",
    "cli-color": "^1.2.0",
    "dependency-graph": "^0.5.0",
    "derequire": "^2.0.6",
    "electron-builder": "19.6.0",
    "electron-rebuild": "1.5.11",
    "fs-extra": "^4.0.2",
    "git-branch": "0.3.0",
    "glob": "^7.1.2",
    "glob-all": "3.1.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.15",
    "moment": "2.18.1",
    "semver": "^5.4.1",
    "semver-sort": "^0.0.4",
    "snazzy": "^7.0.0",
    "spectron": "^3.7.2",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "uglify-js": "^2.7.5",
    "yargs": "^6.6.0",
    "handlebars": "^4.0.8",
    "slack": "^8.3.1"
  },
  "standard": {
    "ignore": [
      "/packages/*"
    ]
  },
  "dependencies": {},
  "build": {
    "appId": "com.Haiku.HaikuForDesignersAndEngineers",
    "copyright": "Copyright (c) 2017 Haiku",
    "productName": "Haiku",
    "forceCodeSigning": true,
    "electronVersion": "1.7.0",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "zip"
    },
    "asar": false,
    "asarUnpack": [
      "index.js",
      "config.js",
      "bins/*",
      "source/plumbing/bins/*",
      "source/plumbing/HaikuHelper.js",
      "source/plumbing/lib/**/*",
      "source/plumbing/package.json",
      "source/plumbing/node_modules/@haiku/cli/*.js",
      "source/plumbing/node_modules/@haiku/cli/lib/**/*",
      "source/plumbing/node_modules/haiku-creator-electron/index.html",
      "source/plumbing/node_modules/haiku-creator-electron/public/**/*",
      "source/plumbing/node_modules/haiku-creator-electron/lib/**/*",
      "source/plumbing/node_modules/haiku-glass/index.html",
      "source/plumbing/node_modules/haiku-glass/public/**/*",
      "source/plumbing/node_modules/haiku-glass/lib/**/*",
      "source/plumbing/node_modules/haiku-timeline/index.html",
      "source/plumbing/node_modules/haiku-timeline/public/**/*",
      "source/plumbing/node_modules/haiku-timeline/lib/**/*",
      "source/plumbing/node_modules/@haiku/player/**/*",
      "source/plumbing/node_modules/haiku-serialization/src/**/*",
      "source/plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/**/*"
    ],
    "files": [
      "**/*",
      "!node_modules${/*}",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj,ts,map}",
      "!**/._*",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,yarn-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!source/**/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!source/plumbing/node_modules/nodegit",
      "!source/plumbing/node_modules/electron",
      "!source/plumbing/node_modules/@haiku/player/demo",
      "!source/plumbing/node_modules/aws-sdk",
      "!source/plumbing/node_modules/cli-table2",
      "!source/plumbing/node_modules/eslint",
      "!source/plumbing/node_modules/haiku-glass/node_modules/monaco-editor/dev",
      "!source/plumbing/node_modules/monaco-editor/dev",
      "!source/plumbing/node_modules/mout",
      "!source/plumbing/node_modules/node-pre-gyp",
      "!source/plumbing/node_modules/react-split-pane/demo",
      "!source/plumbing/node_modules/rocambole-indent",
      "!source/plumbing/node_modules/sinon",
      "!source/plumbing/node_modules/table",
      "source/plumbing/node_modules/nodegit/package.json",
      "source/plumbing/node_modules/nodegit/dist/**",
      "source/plumbing/node_modules/nodegit/build/Release/nodegit.node",
      "!source/plumbing/dev${/*}",
      "!source/plumbing/docs${/*}",
      "!source/plumbing/src${/*}",
      "!source/plumbing/test${/*}",
      "!source/plumbing/node_modules/@haiku/cli/src${/*}",
      "!source/plumbing/node_modules/haiku-creator-electron/src${/*}",
      "!source/plumbing/node_modules/haiku-glass/src${/*}",
      "!source/plumbing/node_modules/@haiku/sdk-client/src${/*}",
      "!source/plumbing/node_modules/haiku-sdk-creator/src${/*}",
      "!source/plumbing/node_modules/@haiku/sdk-inkstone/src${/*}",
      "!source/plumbing/node_modules/haiku-timeline/src${/*}",
      "!source/plumbing/node_modules/haiku-testing${/*}",
      "!source/plumbing/node_modules/haiku-ui-common/src${/*}",
      "!**/node_modules/@haiku/player/dist${/*}",
      "!**/node_modules/@haiku/player/src${/*}",
      "!**/node_modules/@haiku/player/test${/*}",
      "!build${/*}",
      "!changelog${/*}",
      "!packages${/*}",
      "!~tmp${/*}",
      "!scripts${/*}",
      "!_config.js.handlebars",
      "!distro.sh",
      "!README.md",
      "!releases.json",
      "!yarn.lock"
    ]
  }
}
