{
  "name": "haiku-plumbing",
  "version": "3.4.8",
  "description": "Haiku plumbing (serializer, processes, etc)",
  "authors": [
    "Zack Brown <zack@haiku.ai>",
    "Matthew Trost <matthew@haiku.ai>",
    "Taylor Poe <taylor@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "start": "node ./HaikuHelper.js",
    "setup": "./setup.sh",
    "watch": "gulp watch",
    "compile": "gulp compile",
    "test": "yarn run test:git && yarn run test:plumbing && yarn run test:fuzz || true",
    "test:git": "yarn compile && cross-env NODE_ENV=test tape \"test/git/**/*.test.js\" | tap-spec || true",
    "test:plumbing": "yarn compile && cross-env NODE_ENV=test tape \"test/plumbing/**/*.test.js\" | tap-spec || true",
    "test:fuzz": "yarn compile && cross-env NODE_ENV=test tape \"test/fuzz/**/*.test.js\" | tap-spec || true",
    "test-report": "tape \"test/**/*.test.js\" > test-result.tap",
    "lint": "eslint \"src/**/*.js\"",
    "fix": "yarn lint --fix",
    "lint-report": "yarn lint -f checkstyle -o checkstyle-result.xml",
    "repl": "electron ./HaikuHelper.js --folder=../haiku-glass/test/projects/primitives --mode=headless --repl"
  },
  "dependencies": {
    "@haiku/cli": "3.4.8",
    "@haiku/core": "3.4.8",
    "@haiku/sdk-client": "3.4.8",
    "@haiku/sdk-inkstone": "3.4.8",
    "async": "^2.5.0",
    "chokidar": "^1.7.0",
    "dedent": "^0.7.0",
    "dotenv": "^5.0.1",
    "haiku-common": "3.4.8",
    "haiku-formats": "3.4.8",
    "haiku-fs-extra": "3.4.8",
    "haiku-sdk-creator": "3.4.8",
    "haiku-serialization": "3.4.8",
    "lodash": "^4.17.4",
    "lodash.assign": "^4.2.0",
    "lodash.clone": "^4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.throttle": "4.1.1",
    "moment": "2.18.1",
    "nodegit": "0.20.3",
    "qs": "6.4.0",
    "raven": "^2.2.1",
    "rollup": "^0.53.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "semver": "^5.4.1",
    "tmp": "0.0.31",
    "ws": "3.2.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "cross-env": "^5.1.6",
    "electron": "2.0.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "gulp": "3.9.1",
    "gulp-cli": "1.2.2",
    "gulp-watch": "4.3.11",
    "haiku-creator": "3.4.8",
    "leaked-handles": "^5.2.0",
    "longjohn": "^0.2.12",
    "njstrace": "0.0.2",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.0",
    "tmp": "0.0.31"
  }
}
