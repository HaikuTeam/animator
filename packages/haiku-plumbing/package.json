{
  "name": "haiku-plumbing",
  "version": "4.3.0",
  "description": "Haiku plumbing (serializer, processes, etc)",
  "authors": [
    "Zack Brown <zack@haiku.ai>",
    "Matthew Trost <matthew@haiku.ai>",
    "Taylor Poe <taylor@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.all.json -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "ts": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.all.json",
    "test": "yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' | tap-spec",
    "e2e": "yarn ts ./node_modules/.bin/tape 'test/e2e/*.e2e.ts' | tap-spec",
    "test-report": "nyc yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' > test-result.tap",
    "compile": "tsc",
    "compile-declarations": "tsc -p tsconfig.declarations.json",
    "develop": "tsc --watch"
  },
  "dependencies": {
    "@haiku/cli": "4.3.0",
    "@haiku/core": "4.3.0",
    "@haiku/sdk-client": "4.3.0",
    "@haiku/sdk-inkstone": "4.3.0",
    "async": "^2.5.0",
    "chokidar": "^1.7.0",
    "dedent": "^0.7.0",
    "fs-extra": "^4.0.2",
    "haiku-common": "4.3.0",
    "haiku-formats": "4.3.0",
    "haiku-fs-extra": "4.3.0",
    "haiku-sdk-creator": "4.3.0",
    "haiku-serialization": "4.3.0",
    "lodash": "^4.17.4",
    "lodash.assign": "^4.2.0",
    "lodash.clone": "^4.5.0",
    "lodash.debounce": "4.0.8",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.merge": "4.6.0",
    "lodash.throttle": "4.1.1",
    "nodegit": "0.20.3",
    "qs": "6.4.0",
    "raven": "^2.2.1",
    "semver": "^5.4.1",
    "tmp": "0.0.31",
    "ws": "3.2.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/semver": "^5.4.1",
    "@types/fs-extra": "^4.0.8",
    "cross-env": "^5.1.6",
    "electron": "2.0.8",
    "haiku-creator": "4.3.0",
    "istanbul-reporter-cobertura-haiku": "^1.0.2",
    "nyc": "^13.0.1",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.0",
    "ts-node": "^6.1.0",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.11.0",
    "tslint-config-haiku": "^1.0.16",
    "typescript": "^3.0.3",
    "leaked-handles": "^5.2.0",
    "haiku-testing": "4.3.0",
    "spectron": "^3.8.0",
    "tmp": "0.0.31"
  }
}
