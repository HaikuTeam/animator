{
  "name": "haiku-creator",
  "productName": "Haiku",
  "private": true,
  "version": "3.5.2",
  "description": "Haiku Creator UI",
  "main": "index.js",
  "author": "Matthew Trost <matthew@haiku.ai>",
  "license": "PROPRIETARY",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "scripts": {
    "lint": "tslint -p tsconfig.all.json -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "ts": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.all.json",
    "test": "yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' | tap-spec",
    "test-report": "nyc yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' > test-result.tap",
    "compile": "tsc",
    "compile-declarations": "tsc -p tsconfig.declarations.json",
    "develop": "tsc --watch",
    "start": "electron ./lib/electron.js"
  },
  "dependencies": {
    "@haiku/core": "3.5.2",
    "@haiku/taylor-hai": "^0.0.7",
    "@haiku/taylor-ipreview2": "^0.0.2",
    "@haiku/taylor-creatorintro": "^0.0.5",
    "async": "^2.5.0",
    "better-react-spinkit": "2.0.0-4",
    "color": "^2.0.1",
    "electron-proxy-agent": "^1.0.2",
    "event-emitter": "^0.3.5",
    "haiku-common": "3.5.2",
    "haiku-glass": "3.5.2",
    "haiku-sdk-creator": "3.5.2",
    "haiku-serialization": "3.5.2",
    "haiku-timeline": "3.5.2",
    "haiku-ui-common": "3.5.2",
    "inline-style-prefixer": "2.0.5",
    "is-online": "^7.0.0",
    "lodash": "^4.17.4",
    "lodash.assign": "^4.2.0",
    "lodash.debounce": "4.0.8",
    "marked": "^0.3.17",
    "mixpanel": "0.6.0",
    "monaco-editor": "^0.13.1",
    "node-fetch": "^2.0.0-alpha.9",
    "opn": "^5.3.0",
    "qs": "6.4.0",
    "radium": "0.18.1",
    "raven-js": "^3.18.1",
    "react": "15.6.2",
    "react-animations": "0.1.0",
    "react-contextmenu": "2.1.0",
    "react-dom": "15.6.2",
    "react-drag-and-drop": "2.4.0",
    "react-draggable": "2.2.3",
    "react-height": "2.2.0",
    "react-motion": "0.4.7",
    "react-onclickoutside": "^6.4.0",
    "react-popover": "^0.4.18",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.1.1",
    "@types/qs": "^6.5.1",
    "electron": "2.0.2",
    "electron-devtools-installer": "^2.2.4",
    "haiku-testing": "3.5.2"
  }
}
