{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "curly": true,
    "interface-name": false,
    "max-line-length": [
      true,
      120
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "function-name": false,
    "no-bitwise": false,
    "no-increment-decrement": false,
    "object-curly-spacing": [
      true,
      "never"
    ],
    "strict-boolean-expressions": false,
    "ter-arrow-parens": [
      true,
      "always"
    ]
  }
}
