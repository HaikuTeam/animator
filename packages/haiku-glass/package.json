{
  "name": "haiku-glass",
  "version": "3.4.8",
  "private": true,
  "authors": [
    "Matthew Trost <matthew@haiku.ai>",
    "Zack Brown <zack@haiku.ai>",
    "Taylor Poe <taylor@haiku.ai>"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "standard": {
    "ignore": [
      "./src/react/modes/haiku.js"
    ]
  },
  "dependencies": {
    "@haiku/core": "3.4.8",
    "babel-register": "6.26.0",
    "bluebird": "^3.5.0",
    "capitalize": "^1.0.0",
    "color": "^2.0.1",
    "cst": "^0.4.10",
    "haiku-common": "3.4.8",
    "haiku-fs-extra": "3.4.8",
    "haiku-serialization": "3.4.8",
    "haiku-ui-common": "3.4.8",
    "lodash": "^4.17.4",
    "moment": "2.18.1",
    "monaco-editor": "^0.12.0",
    "prettier": "^0.22.0",
    "pretty": "^2.0.0",
    "qs": "6.4.0",
    "radium": "0.18.1",
    "raven-js": "^3.18.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-selectize": "^3.0.1",
    "react-transition-group": "^1.1.2",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "async": "^2.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^2.2.0",
    "cross-env": "^5.1.6",
    "electron": "2.0.2",
    "eslint": "^3.19.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "jsdom": "^11.1.0",
    "prop-types": "^15.6.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.0"
  },
  "scripts": {
    "start": "cross-env AUTOSTART=main electron --enable-logging --remote-debugging-port=9222 ./lib/electron.js",
    "watch": "babel --watch src --out-dir lib --source-maps inline",
    "compile": "cross-env NODE_ENV=development babel src --out-dir lib --source-maps inline",
    "lint": "eslint \"src/**/*.js\"",
    "fix": "yarn lint --fix",
    "lint-report": "yarn lint -f checkstyle -o checkstyle-result.xml",
    "test": "cross-env NODE_ENV=development yarn compile && tape \"test/**/*.test.js\" | tap-spec || true",
    "test-report": "tape \"test/**/*.test.js\" > test-result.tap"
  }
}
