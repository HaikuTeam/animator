  "scripts": {
    "lint": "tslint -p tsconfig.all.json -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "ts": "cross-env NODE_ENV=test ts-node -r tsconfig-paths/register -P tsconfig.all.json",
    "test": "yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' | tap-spec",
    "test-report": "nyc yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' 'test/**/*.test.js' > test-result.tap",
    "compile": "tsc",
    "compile-declarations": "tsc -p tsconfig.declarations.json",
    "develop": "tsc --watch",
    "start": "cross-env AUTOSTART=main electron --enable-logging --remote-debugging-port=9222 ./lib/electron.js"
  },
  "dependencies": {
    "@haiku/core": "3.5.2",
    "balloon-css": "^0.5.0",
    "classnames": "2.2.5",
    "codemirror": "^5.28.0",
    "color": "^2.0.1",
    "haiku-common": "3.5.2",
    "haiku-fs-extra": "3.5.2",
    "haiku-serialization": "3.5.2",
    "haiku-ui-common": "3.5.2",
    "lodash": "^4.17.4",
    "qs": "6.4.0",
    "radium": "0.18.1",
    "raven-js": "^3.18.1",
    "react": "15.6.2",
    "react-beautiful-dnd": "^6.0.1",
    "react-dom": "15.6.2",
    "react-draggable": "2.2.3",
    "react-intercom": "^1.0.14",
    "react-popover": "^0.4.18",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.6",
    "electron": "2.0.7",
    "haiku-testing": "3.5.2",
    "istanbul-reporter-cobertura-haiku": "^1.0.1",
    "jsdom": "^11.1.0",
    "leaked-handles": "^5.2.0",
    "nyc": "^11.8.0",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.0",
    "ts-node": "^6.1.0",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.10.0",
    "tslint-config-haiku": "^1.0.15",
    "typescript": "^2.9.1"
  }
