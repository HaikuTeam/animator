{
  "name": "haiku-timeline",
  "version": "3.0.49",
  "private": true,
  "license": "UNLICENSED",
  "authors": [
    "Matthew Trost <matthew@haiku.ai>",
    "Taylor Poe <taylor@haiku.ai>",
    "Zack Brown <zack@haiku.ai>"
  ],
  "scripts": {
    "start": "AUTOSTART=main electron --enable-logging --remote-debugging-port=9222 ./lib/electron.js",
    "watch": "NODE_ENV=development babel --watch src --out-dir lib --source-maps inline",
    "compile": "NODE_ENV=development babel src --out-dir lib --source-maps inline",
    "test": "NODE_ENV=development yarn compile && tape \"test/**/*.test.js\" | tap-spec",
    "test-report": "tape \"test/**/*.test.js\" > test-result.tap",
    "lint": "eslint \"src/**/*.js\"",
    "fix": "yarn lint --fix",
    "lint-report": "yarn lint -f checkstyle -o checkstyle-result.xml"
  },
  "standard": {
    "ignore": [
      "./src/components/modes/haiku.js",
      "./src/components/modes/haiku-hint.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@haiku/core": "3.0.49",
    "archy": "^1.0.0",
    "classnames": "2.2.5",
    "codemirror": "^5.28.0",
    "color": "^2.0.1",
    "decamelize": "^1.2.0",
    "haiku-bytecode": "3.0.49",
    "haiku-common": "3.0.49",
    "haiku-fs-extra": "3.0.49",
    "haiku-serialization": "3.0.49",
    "haiku-ui-common": "3.0.49",
    "jsdom": "^11.1.0",
    "lodash": "^4.17.4",
    "numeral": "^2.0.6",
    "qs": "6.4.0",
    "radium": "0.18.1",
    "raven-js": "^3.18.1",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-draggable": "2.2.3",
    "react-popover": "^0.4.18",
    "react-scroll-to-component": "^1.0.1",
    "strip-indent": "^2.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^2.2.0",
    "electron": "1.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  }
}
