{
  "name": "haiku-sdk-creator",
  "version": "2.3.41",
  "description": "SDK into Haiku",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json --type-check --format stylish",
    "fix": "yarn lint --fix",
    "test": "tsc && tsc -p tsconfig.test.json && NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec && rm -rf ~test",
    "coverage": "nyc yarn test",
    "html-coverage": "nyc --reporter=html yarn test",
    "compile": "tsc",
    "develop": "tsc --watch"
  },
  "authors": [
    "Zack Brown <zack@haiku.ai>",
    "Matthew Trost <matthew@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "bluebird": "^3.5.0",
    "@haiku/sdk-client": "HaikuTeam/sdk-client.git",
    "@haiku/sdk-inkstone": "HaikuTeam/sdk-inkstone.git",
    "haiku-serialization": "HaikuTeam/serialization",
    "npm": "^5.3.0",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.17",
    "@types/ws": "^3.0.2",
    "leaked-handles": "^5.2.0",
    "nyc": "^11.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.4.2"
  }
}
