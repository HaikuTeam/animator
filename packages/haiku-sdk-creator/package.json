{
  "name": "haiku-sdk-creator",
  "version": "3.2.23",
  "description": "SDK into Haiku",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts' -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "test": "tsc && tsc -p tsconfig.test.json && cross-env NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec && rm -rf ~test",
    "test-report": "tsc -p tsconfig.test.json && cross-env NODE_ENV=test tape \"~test/**/*.test.js\" > test-result.tap",
    "coverage": "nyc yarn test",
    "html-coverage": "nyc --reporter=html yarn test",
    "compile": "tsc",
    "develop": "tsc --watch"
  },
  "authors": [
    "Zack Brown <zack@haiku.ai>",
    "Matthew Trost <matthew@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@haiku/sdk-client": "3.2.23",
    "@haiku/sdk-inkstone": "3.2.23",
    "bluebird": "^3.5.0",
    "haiku-serialization": "3.2.23",
    "qs": "6.4.0",
    "ws": "3.2.0"
  },
  "devDependencies": {
    "@types/node": "8.9.4",
    "@types/ws": "^3.0.2",
    "@types/bluebird": "^3.5.20",
    "cross-env": "^5.1.4",
    "leaked-handles": "^5.2.0",
    "nyc": "^11.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.5.2"
  }
}
