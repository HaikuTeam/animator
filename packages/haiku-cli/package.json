{
  "name": "haiku-cli",
  "version": "2.3.8",
  "description": "Haiku CLI",
  "main": "lib/index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0",
    "yarn": "1.0.2",
    "electron": "1.7.0"
  },
  "scripts": {
    "develop": "tsc --watch",
    "transpile": "tsc",
    "tsc": "tsc",
    "lint": "tslint --type-check --project tsconfig.json -c tslint.json 'src/**/*.ts' --format stylish --fix",
    "test": "rm -rf ~test || true && tsc && tsc -p tsconfig.test.json && NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec"
  },
  "authors": [
    "Zack Brown <zack@haiku.ai>",
    "Matthew Trost <matthew@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "async": "^2.5.0",
    "chalk": "^1.1.3",
    "cli-color": "^1.1.0",
    "dedent": "^0.6.0",
    "fs-extra": "^4.0.2",
    "haiku-sdk-client": "HaikuTeam/sdk-client.git",
    "haiku-sdk-inkstone": "HaikuTeam/sdk-inkstone.git",
    "hasbin": "^1.2.3",
    "inquirer": "^2.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "prepend-file": "^1.3.1",
    "request": "^2.79.0",
    "semver": "^5.4.1",
    "tail": "^1.2.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/cli-color": "^0.3.29",
    "@types/inquirer": "0.0.32",
    "@types/lodash": "^4.14.45",
    "@types/mkdirp": "^0.3.29",
    "@types/node": "^6.0.56",
    "@types/request": "^2.0.0",
    "@types/yargs": "^6.5.0",
    "try-thread-sleep": "^1.0.0",
    "typescript": "^2.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "tslint": "^5.7.0"
  }
}
