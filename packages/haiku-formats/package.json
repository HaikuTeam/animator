{
  "name": "haiku-formats",
  "version": "3.3.4",
  "description": "Formatting-related utilities for Haiku.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts' -t stylish",
    "lint-test": "tslint -p tsconfig.test.json -c tslint.json 'test/**/*.ts' -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "test": "tsc && tsc -p tsconfig.test.json && cross-env NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec && rm -rf ~test",
    "test-report": "tsc -p tsconfig.test.json && cross-env NODE_ENV=test tape \"~test/**/*.test.js\" > test-result.tap",
    "coverage": "nyc yarn test",
    "html-coverage": "nyc --reporter=html yarn test",
    "compile": "tsc",
    "develop": "tsc --watch",
    "regenerate-goldens": "tsc && node ./scripts/regenerate-goldens.js",
    "upgrade-goldens": "tsc && node ./scripts/upgrade-goldens.js",
    "demos": "node ./scripts/demos.js"
  },
  "authors": [
    "Sasha Joseph <sasha@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@haiku/core": "3.3.4",
    "bezier-easing": "^2.0.3",
    "haiku-common": "3.3.4",
    "haiku-sdk-creator": "3.3.4",
    "haiku-serialization": "3.3.4",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bezier-easing": "^2.0.0",
    "@types/lodash": "^4.14.108",
    "@types/node": "8.9.4",
    "@types/tape": "^4.2.30",
    "async": "^2.5.0",
    "cross-env": "^5.1.4",
    "haiku-fs-extra": "3.3.4",
    "haiku-testing": "3.3.4",
    "lottie-web": "5.1.7",
    "nodemon": "1.11.0",
    "nyc": "^11.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.5.2",
    "walk-sync": "^0.3.2",
    "webpack": "^3.8.1",
    "webpack-concat-plugin": "^2.4.2"
  }
}
