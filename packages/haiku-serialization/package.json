{
  "name": "haiku-serialization",
  "version": "3.5.1",
  "description": "Serialization utilities",
  "main": "index.js",
  "scripts": {
    "test": "yarn test:all",
    "test:all": "cross-env NODE_ENV=test tape \"test/**/**/*.test.js\" | tap-spec || true",
    "test:utils": "cross-env NODE_ENV=test tape \"test/utils/**/*.test.js\" | tap-spec || true",
    "test:bll": "cross-env NODE_ENV=test tape \"test/bll/**/*.test.js\" | tap-spec || true",
    "test-report": "cross-env NODE_ENV=test tape \"test/**/**/*.test.js\" > test-result.tap",
    "lint": "eslint \"src/**/*.js\"",
    "fix": "yarn lint --fix",
    "lint-report": "yarn lint -f checkstyle -o checkstyle-result.xml"
  },
  "author": "Matthew Trost <matthew@haiku.ai>",
  "license": "UNLICENSED",
